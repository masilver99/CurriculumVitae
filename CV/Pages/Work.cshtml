@page
@model CV.Pages.WorkModel
@{
    ViewData["Title"] = "work";
}

@foreach (var workItem in Model.WorkItems)
{
    <div>
        <h1>@workItem.CompanyName</h1>
    </div>
    <div>
        @workItem.Dates
    </div>
    <div>
        @workItem.Location
    </div>
    <div>
        @workItem.Position
    </div>
    <ul>
        @foreach (var item in workItem.BulletPoints)
        {
            <li>
                @item
            </li>
        }
    </ul>
}

